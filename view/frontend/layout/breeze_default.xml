<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="meilisearch" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/lib/meilisearch.umd.js</item>
                                <item name="global" xsi:type="string">MeiliSearch</item>
                            </item>
                            <item name="noUiSlider" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/lib/nouislider.min</item>
                                <item name="global" xsi:type="string">noUiSlider</item>
                            </item>
                            <item name="fuse" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/lib/fuse.min</item>
                                <item name="global" xsi:type="string">Fuse</item>
                            </item>
                            <!-- Services -->
                            <item name="Walkwizus_MeilisearchFrontend/js/service/meilisearch-service" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/service/meilisearch-service</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/service/query-builder" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/service/query-builder</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/service/search" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/service/search</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <!-- Components -->
                            <item name="Walkwizus_MeilisearchFrontend/js/components/autocomplete" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/autocomplete</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/autocomplete/product" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/autocomplete/product</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/boot" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/boot</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/counter" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/counter</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/facets" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/facets</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/facets/searchbox" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/facets/searchbox</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/facets/types/checkbox" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/facets/types/checkbox</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/facets/types/price" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/facets/types/price</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/hits" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/hits</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/limiter" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/limiter</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/pagination" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/pagination</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/sorter" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/sorter</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/url" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/url</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/components/viewmode" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/components/viewmode</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <!-- Models -->
                            <item name="Walkwizus_MeilisearchFrontend/js/model/config-model" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/config-model</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/facets-state" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/facets-state</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/limiter-state" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/limiter-state</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/product-model" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/product-model</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/search-state" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/search-state</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/sorter-state" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/sorter-state</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/model/viewmode-state" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/model/viewmode-state</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/prices" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/prices</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="Walkwizus_MeilisearchFrontend/js/swatches" xsi:type="array">
                                <item name="path" xsi:type="string">Walkwizus_MeilisearchFrontend/js/swatches</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="breeze.container">
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::autocomplete.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::autocomplete/category.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::autocomplete/product.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::counter.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::facets.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::facets/searchbox.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::facets/types/checkbox.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::facets/types/price.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::facets/types/swatch.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::hits.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::limiter.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::pagination.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::sorter.html"/>
            <block class="Swissup\Breeze\Block\HtmlTemplate" template="Walkwizus_MeilisearchFrontend::viewmode.html"/>
        </referenceContainer>
    </body>
</page>
